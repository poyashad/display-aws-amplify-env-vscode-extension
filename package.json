{
  "name": "display-aws-amplify-environment",
  "displayName": "Display AWS Amplify Environment",
  "description": "This extension displays the AWS Amplify environment. It makes it easy for you to see which environment you are currently working in",
  "publisher": "poyashad",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poyashad/display-aws-amplify-env-vscode-extension.git"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/team-provider-info.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "display-aws-amplify-environment.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "display-aws-amplify-environment": "github:poyashad/display-aws-amplify-env-vscode-extension"
  }
}
